{
  "name": "helsingborg-stad/apiv2",
  "description": "Version 2 of helsingborg stad API!",
  "license": "MIT",
  "authors": [
    {
      "name": "Helsingborg Stad",
      "email": "webbredaktionen@helsingborg.se"
    }
  ],
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "wpackagist-plugin/redis-cache": "^2.3.0",
    "helsingborg-stad/better-post-ui": "^1.0.0",
    "helsingborg-stad/force-ssl": "^1.0.0",
    "johnpbloch/wordpress": "^6.0.0",
    "wpackagist-plugin/classic-editor": "^0.4.0",
    "wpackagist-plugin/wp-rest-api-log": "^1.6.9",
    "wpackagist-plugin/wp-rest-filter": "^1.4.3",
    "helsingborg-stad/api-volunteer-manager": "^0.1.0"
  },
  "extra": {
    "path": "/",
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "wp-content/themes/{$name}": [
        "type:wordpress-theme"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "johnpbloch/wordpress-core-installer": true
    }
  }
}
