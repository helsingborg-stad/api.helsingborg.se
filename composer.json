{
  "name": "helsingborg-stad/apiv2",
  "description": "Version 2 of helsingborg stad API!",
  "license": "MIT",
  "authors": [
    {
      "name": "Helsingborg Stad",
      "email": "webbredaktionen@helsingborg.se"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "wpackagist-plugin/redis-cache": "^2.3.0",
    "helsingborg-stad/better-post-ui": "^1.0.0",
    "helsingborg-stad/force-ssl": "^1.0.0",
    "johnpbloch/wordpress": "^6.5.2",
    "bueltge/must-use-loader": "^1.3",
    "wpackagist-plugin/classic-editor": "^0.4.0",
    "wpackagist-plugin/wp-rest-api-log": "^1.6.9",
    "wpackagist-plugin/wp-rest-filter": "^1.4.3",
    "wpackagist-plugin/simple-smtp": "^1.3.2",
    "helsingborg-stad/api-volunteer-manager": "^0.3.9",
    "helsingborg-stad/active-directory-api-wp-integration": "^3.0",
    "helsingborg-stad/api-alarm-manager": ">=1.2.1",
    "wpackagist-plugin/litespeed-cache": "^5.3.0",
    "helsingborg-stad/api-schools-manager": "~0.22.1",
    "helsingborg-stad/wpmu-acf-google-maps-key": "^1.0",
    "helsingborg-stad/wpmu-network-admin-url": "^2.0",
    "wpackagist-plugin/acf-extended": "0.8.9.5",
    "helsingborg-stad/webhooks-manager": "^1.3",
    "helsingborg-stad/api-event-manager": "^3.6.1",
    "helsingborg-stad/modularity": "^6.13",
    "helsingborg-stad/municipio": "^4.23"
  },
  "extra": {
    "path": "/",
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "wp-content/mu-plugins": [
        "bueltge/must-use-loader"
      ],
      "wp-content/mu-plugins/{$name}": [
        "wpackagist-plugin/simple-smtp",
        "type:wordpress-muplugin"
      ],
      "wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "wp-content/themes/{$name}": [
        "type:wordpress-theme"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "johnpbloch/wordpress-core-installer": true,
      "kylekatarnls/update-helper": true
    }
  }
}
