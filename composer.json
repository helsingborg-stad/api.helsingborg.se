{
  "name": "helsingborg-stad/apiv2",
  "description": "Version 2 of helsingborg stad API!",
  "license": "MIT",
  "authors": [
    {
      "name": "Helsingborg Stad",
      "email": "webbredaktionen@helsingborg.se"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "wpackagist-plugin/redis-cache": "^2.3.0",
    "helsingborg-stad/better-post-ui": "^1.0.0",
    "helsingborg-stad/force-ssl": "^1.0.0",
    "johnpbloch/wordpress": "^6.0.0",
    "bueltge/must-use-loader": "^1.3",
    "wpackagist-plugin/classic-editor": "^0.4.0",
    "wpackagist-plugin/wp-rest-api-log": "^1.6.9",
    "wpackagist-plugin/wp-rest-filter": "^1.4.3",
    "wpackagist-plugin/simple-smtp": "^1.3.2",
    "helsingborg-stad/api-volunteer-manager": "dev-master",
    "helsingborg-stad/active-directory-api-wp-integration": "^2.4.1",
    "helsingborg-stad/api-alarm-manager": ">=1.2.1",
    "helsingborg-stad/municipio": "dev-3.0/develop"
  },
  "extra": {
    "path": "/",
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "wp-content/mu-plugins/{$name}": [
        "wpackagist-plugin/simple-smtp",
        "type:wordpress-muplugin"
      ],
      "wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "wp-content/themes/{$name}": [
        "type:wordpress-theme"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
        "@php post-install.php"
    ],
    "post-update-cmd": [
        "@php post-install.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "johnpbloch/wordpress-core-installer": true
    }
  }
}
